<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
<!-- 객체생성(setter방식) -->
<bean id="p1" class="kr.or.iei.person.model.vo.Person">
	<property name="name" value="이름1"></property>
	<property name="age"> 
		<!-- 정수형 대입법 -->
		<value type="java.lang.Integer">11</value>
	</property>
	<property name="addr" value="주소1"></property>
</bean>
<!-- 객체생성(생성자방식) -->
<bean id="p2" class="kr.or.iei.person.model.vo.Person">
	<constructor-arg value="이름2"/>
	<constructor-arg>
		<value type="java.lang.Integer">22</value>
	</constructor-arg>
	<constructor-arg value="주소2"/>
</bean>
<bean id="pm" class="kr.or.iei.person.model.vo.PersonMgr">
	<!-- ref 참조할때 사용 -->
	<property name="p" ref="p2"></property>
</bean>
</beans>
